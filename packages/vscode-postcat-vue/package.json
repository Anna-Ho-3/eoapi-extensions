{
  "name": "vscode-postcat-vue",
  "title": "Vue Theme",
  "version": "0.0.1",
  "description": "Vue Theme",
  "author": {
    "name": "Postcat"
  },
  "engines": {
    "postcat": "^0.0.1"
  },
  "categories": [
    "Themes"
  ],
  "features": {
    "theme": [
      {
        "label": "${theme.label}",
        "baseTheme": "pc-dark",
        "id": "vscode-vue",
        "path": "./themes/default.json"
      }
    ],
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  }
}
