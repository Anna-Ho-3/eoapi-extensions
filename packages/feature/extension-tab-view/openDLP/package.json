{
  "name": "eoapi-opendlp",
  "title": "OpenDLP sercurity check",
  "version": "0.0.1",
  "description": "OpenDLP detect API sensitive words to make your API more secure",
  "repository": {
    "url": "https://github.com/hitsz-ids/openDLP"
  },
  "main": "src/operate.js",
  "node": "src/main.js",
  "author": "OpenDLP",
  "logo": "https://avatars.githubusercontent.com/u/55747983?s=200&v=4",
  "scripts": {
    "build": "webpack src/index.js"
  },
  "features": {
    "extensionTabView": [
      {
        "name": "Operate",
        "url": "./page/index.html",
        "debugUrl": "http://localhost:8080"
      }
    ],
    "apiPreviewTab": {
      "rightExtra": [
        {
          "name": "API Sercurity Check",
          "icon": "file-text-one",
          "tooltip": "扫描API敏感词",
          "tooltipTrigger": "hover",
          "events": [
            {
              "name": "click",
              "handler": "sercurityCheck"
            }
          ]
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "API Sercurity",
      "properties": {
        "serverUrl": {
          "type": "string",
          "required": true,
          "default": "",
          "label": "OpenDLP服务地址",
          "description": "部署OpenDLP服务后得到的地址"
        }
      }
    }
  },
  "dependencies": {
    "@grpc/grpc-js": "~1.1.8",
    "google-protobuf": "^3.21.2"
  }
}
