{
  "name": "eoapi-opendlp",
  "title": "OpenDLP sercurity check",
  "version": "0.0.1",
  "description": "OpenDLP detect API sensitive words to make your API more secure",
  "repository": {
    "url": "https://github.com/hitsz-ids/openDLP"
  },
  "node": "src/main.js",
  "author": "OpenDLP",
  "logo": "https://avatars.githubusercontent.com/u/55747983?s=200&v=4",
  "scripts": {
    "build": "webpack src/index.js"
  },
  "features": {
    "configuration": {
      "type": "object",
      "properties": {
        "uri": {
          "type": "string",
          "required": true,
          "default": "",
          "title": "URL",
          "description": "${configuration.url.description}"
        }
      }
    },
    "extensionTabView": [
      {
        "name": "Operate",
        "url": "http://localhost:8080"
      }
    ],
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  },
  "dependencies": {
    "@grpc/grpc-js": "~1.1.8",
    "google-protobuf": "^3.21.2"
  }
}
