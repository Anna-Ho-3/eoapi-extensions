{
  "name": "eoapi-export-openapi",
  "version": "1.0.16",
  "description": "Export the JSON format file of the OpenAPI specification",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "moduleID": "eoapi-feature-export-openapi",
  "moduleName": "Export OpenAPI",
  "moduleType": "feature",
  "logo": "https://raw.githubusercontent.com/eolinker/eoapi-extensions/main/packages/feature/export/openapi/assets/logo.png",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eolinker/eoapi-extensions.git"
  },
  "keywords": [
    "feature",
    "eoapi"
  ],
  "author": "Eoapi",
  "license": "Apache-2.0 License",
  "bugs": {
    "url": "https://github.com/eolinker/eoapi-extensions/issues"
  },
  "homepage": "https://github.com/eolinker/eoapi-extensions#readme",
  "devDependencies": {
    "@types/lodash-es": "*",
    "@types/node": "^17.0.29",
    "esbuild": "^0.14.38",
    "nodemon": "^2.0.15",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.3"
  },
  "features": {
    "apimanage.export": {
      "action": "export_convert",
      "label": "OpenAPI(.json)",
      "description": "${function.description}",
      "icon": "https://raw.githubusercontent.com/eolinker/eoapi-extensions/main/packages/feature/export/openapi/assets/logo.png",
      "filename": "openapi.json"
    },
    "i18n": {
      "sourceLocale": "en-US",
      "locales": [
        "zh-Hans"
      ]
    }
  },
  "dependencies": {
    "lodash": "*",
    "lodash-es": "*",
    "lodash-unified": "^1.0.2"
  }
}
