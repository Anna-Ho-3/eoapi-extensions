{
  "name": "eoapi-push-eolink",
  "version": "1.0.12",
  "description": "eoapi extension for push api data to eolinker",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "moduleID": "eoapi-feature-push-eolink",
  "moduleName": "Push to Eolink",
  "moduleType": "feature",
  "logo": "https://raw.githubusercontent.com/eolinker/eoapi-extensions/main/packages/feature/push/eolink/assets/logo.png",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -w -c rollup.config.js",
    "test": "nodemon index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eolinker/eoapi-extensions.git"
  },
  "keywords": [
    "feature",
    "eoapi"
  ],
  "author": "Eolink",
  "license": "Apache-2.0 License",
  "bugs": {
    "url": "https://github.com/eolinker/eoapi-extensions/issues"
  },
  "homepage": "https://github.com/eolinker/eoapi-extensions#readme",
  "devDependencies": {
    "@types/lodash-es": "*",
    "esbuild": "^0.14.38",
    "nodemon": "^2.0.15",
    "rollup": ">=2.70.0 <3.0.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6"
  },
  "dependencies": {
    "lodash": "*",
    "lodash-es": "*",
    "lodash-unified": "^1.0.2"
  },
  "features": {
    "apimanage.sync": {
      "action": "sync_to_remote",
      "label": "Eolink",
      "description": "Push API data to eolink.",
      "icon": "https://raw.githubusercontent.com/eolinker/eoapi-extensions/main/packages/feature/push/eolink/assets/logo.png",
      "extestion": ""
    }
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Push to Eolink",
      "properties": {
        "eolink.remoteServer.url": {
          "type": "string",
          "required": true,
          "default": "http://127.0.0.2",
          "label": "远程服务器地址",
          "description": "从远程服务器存取数据，配置后支持多人协作"
        },
        "eolink.remoteServer.token": {
          "type": "string",
          "required": false,
          "default": "VUYGIUGUYGHVIHBUGIUHIUFGUIGUFU",
          "label": "Token",
          "description": "远程服务器 Token，配置后才可访问"
        },
        "eolink.remoteServer.projectId": {
          "type": "string",
          "required": false,
          "default": "ProjectID",
          "label": "ProjectID",
          "description": "远程服务器 ProjectID"
        }
      }
    }
  }
}